<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.numbergame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".levels"
            android:label="Levels" />
        <activity
            android:name=".Addition"
            android:label="Addition" />
        <activity
            android:name=".Subtraction"
            android:label="Subtraction" />
        <activity
            android:name=".Multiplication"
            android:label="Multiplication" />
        <activity android:name=".Division"
            android:label="Division">
        </activity>
    </application>

</manifest>